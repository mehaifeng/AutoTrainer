<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="AutoTrainer.App"
             xmlns:local="using:AutoTrainer"
			 xmlns:themes="clr-namespace:Material.Styles.Themes;assembly=Material.Styles"
			 xmlns:avalonia="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
             RequestedThemeVariant="Default">
	    <Application.Resources>
        <FontFamily x:Key="NunitoFont">avares://AutoTrainer/Assets/Fonts#思源黑体 CN Bold</FontFamily>
    </Application.Resources>
	<!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
	    <avalonia:MaterialIconStyles/>
		<themes:MaterialTheme BaseTheme="Light" PrimaryColor="Blue" SecondaryColor="Lime"/>
    </Application.Styles>
</Application>